{
    "parameters" : ["x0=0.2*(xmin+xmax)", 
                    "z0=0.2*(zmin+zmax)", 
                    "lambda=0.0006", 
                    "V0=2000.0"],

    "init" : {"velocity" : {"formula" : "V0"},
              "pressure_0" : {"formula" : "exp(-lambda*((z-z0)^2))"},
              "pressure_1" : {"formula" : "pressure_0"}},

    "grid" : {"file" : "./data/marm2_downsampled_8.xyz"},

    "output" : [ {"type" : "EvalVariable", 
                  "rhythm" : 100, 
                  "name": "pressure_ref",
                  "formula" : "pressure_1 - 0.5 * (exp(-lambda*((z-z0-V0*t)^2)) + exp(-lambda*((z-z0+V0*t)^2)))"},
                 {"type" : "CheckVariables",
                  "rhythm" : 100},
                 {"type" : "OutputVariables", 
                  "rhythm" : 200, 
                  "format": "binary",
                  "file" : "./output/foobar"},
                 {"type" : "OutputVariables", 
                  "rhythm" : 100, 
                  "format": "VTK",
                  "file" : "./output/pressure"},
                 {"type" : "OutputReceivers", 
                  "rhythm" : 10, 
                  "format": "binary",
                  "iz" : 200,
                  "file" : "./output/receivers.dat"}]

}
