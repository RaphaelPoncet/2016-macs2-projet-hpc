{

    "parameters" : ["x0=0.2*(xmin+xmax)", 
                    "z0=0.2*(zmin+zmax)", 
                    "lambda=0.01*nyquist_z"],

    "grid" : {"file" : "./data/marmousi_smooth.xyz"},

    "init" : {"velocity" : {"file" : "./data/marmousi_smooth.xyz"},
              "pressure_0" : {"formula" : "exp(-lambda*((x-x0)^2+(z-z0)^2))"},
              "pressure_1" : {"formula" : "pressure_0"}},

    "output" : [ {"type" : "CheckVariables",
                  "rhythm" : 1000},
                 {"type" : "OutputVariables", 
                  "rhythm" : 1000, 
                  "format": "binary",
                  "file" : "./output/binary_out_%i.dat"},
                 {"type" : "OutputVariables", 
                  "rhythm" : 1000, 
                  "format": "VTK",
                  "file" : "./output/vtk_out_%i.dat"},
                 {"type" : "OutputReceivers", 
                  "rhythm" : 10, 
                  "format": "binary",
                  "iz" : 100,
                  "file" : "./output/receivers.dat"}]

}
